<%- if controller_name != 'sessions' %>
  <%= link_to "Voltar", new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Esqueceu sua senha?", "#", onclick: "alert('Por favor, entre em contato com o suporte para recuperar sua senha.')", class:"block text-sm text-gray-800" %>
<% end %>


